{
  "name": "react-resizable-rotatable-draggable",
  "version": "1.0.0",
  "description": "A rectangle react component which can be resized and rotated",
  "author": "MockingBot",
  "license": "MIT",
  "repository": "github:mockingbot/react-resizable-rotatable-draggable",
  "keywords": [
    "react",
    "rectangle",
    "resize",
    "rotate"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "scripts": {
    "build": "rollup -c",
    "build-dev": "rollup -c -w",
    "build-example": "cd example && npm install && npm run build",
    "publish-dev": "npm publish --tag dev",
    "start": "npm run build-dev",
    "deploy": "npm run build && npm run build-example && gh-pages -d example/dist",
    "prepack": "npm run build",
    "release": "standard-version && git push --follow-tags",
    "publish-package": "npm run build && npm publish"
  },
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^18"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^5.0.0",
    "prop-types": "^15.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-styles": "^4.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {},
  "publishConfig": {
    "@sportvot:registry": "https://npm.pkg.github.com"
  }
}
